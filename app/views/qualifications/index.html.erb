
<h3><span class="label label-default"> Mis Calificaciones </h3>

<table class="table">
  <tr>
    <th>Tipo de Couch</th>
    <th>Ubicacion del Couch</th>
    <th>Calificacion</th>
  </tr>
 
  <% @miscalif.each do |article| %>
    <tr>
      <td><%= Tipoc.find(Couch.find(article.couch_id).tipo).tipo %></td>
      <td><%= Couch.find(article.couch_id).ubicacion %></td>
      <td><%= article.percentage %></td>
    </tr>
  <% end %>
  <% if @miscalif.size == 0 %>
    <tr> <td colspan="3"> No hay Calificaciones </td> </tr>
  <% end %>

</table>




